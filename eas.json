{
    "build": {
      "preview": {
        "android": {
          "buildType": "apk"
        },
        "env": {
          "FIREBASE_KEY": "$FIREBASE_KEY",
          "MAP_API_KEY": "$MAP_API_KEY"
        }
      },
      "preview2": {
        "android": {
          "gradleCommand": ":app:assembleRelease"
        },
        "env": {
          "FIREBASE_KEY": "$FIREBASE_KEY",
          "MAP_API_KEY": "$MAP_API_KEY"
        }
      },
      "preview3": {
        "developmentClient": true,
        "env": {
          "FIREBASE_KEY": "$FIREBASE_KEY",
          "MAP_API_KEY": "$MAP_API_KEY"
        }
      },
      "production": {
        "env": {
          "FIREBASE_KEY": "$FIREBASE_KEY",
          "MAP_API_KEY": "$MAP_API_KEY"
        }
      }
    }
  }
  